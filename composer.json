{
    "name": "juanparati/laravel-kickbox",
    "description": "A Kickbox library for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Lago",
            "email": "juanparati@gmail.com"
        }
    ],
    "require": {
        "illuminate/cache": "^7 || ^8",
        "illuminate/support": "^7 || ^8",
        "php": ">=7.3",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9",
        "orchestra/testbench": "^5.2 || ^6.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juanparati\\LaravelKickbox\\Providers\\KickboxServiceProvider"
            ]
        },
        "aliases": {
            "Kickbox": "Juanparati\\LaravelKickbox\\Facades\\KickboxFacade"
        }
    },
    "autoload": {
        "psr-4": {
            "Juanparati\\LaravelKickbox\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juanparati\\LaravelKickbox\\Tests\\": "tests"
        }
    }
}
